	<ThingDef ParentName="BuildingBase">
		<defName>FuelProduction</defName>
		<label>MISPP</label>
		<description>The Mars In-Situ Propellant Plant (MISPP) manufactures methane propellant from H2O and CO2 via the Sabatier process, must be placed outside to extract co2 from the atmosphere.</description>
		<thingClass>MarsX.Building_FuelProduction</thingClass>
		<graphicData>
			<texPath>Things/Building/mispp</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(5,4)</drawSize>
			<damageData>
				<rect>(0.05,0.05,0.95,0.95)</rect>
			</damageData>
		</graphicData>
		<category>Building</category>
		<tickerType>Normal</tickerType>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<blockWind>true</blockWind>
		<castEdgeShadows>true</castEdgeShadows>
		<selectable>true</selectable>
		<fillPercent>0.20</fillPercent>
		<pathCost>8</pathCost>
		<size>(3,2)</size>
		<costList>
			<Steel>150</Steel>
			<Aluminium>45</Aluminium>
			<ComponentIndustrial>6</ComponentIndustrial>
		</costList>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<designationCategory>Ship</designationCategory>
		<rotatable>false</rotatable>
		<statBases>
			<MaxHitPoints>350</MaxHitPoints>
			<WorkToBuild>1000</WorkToBuild>
			<Flammability>0.0</Flammability>
			<Beauty>-12</Beauty>
		</statBases>
		<comps>
			<li Class="MarsX.CompProperties_Pipe">
				<mode>Gas</mode>
			</li>
			<li Class="DubsBadHygiene.CompProperties_Pipe">
				<mode>Sewage</mode>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>750</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
	</ThingDef>

</Defs>