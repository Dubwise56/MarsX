	<ThingDef ParentName="MinifiedBuildingBase">
		<defName>o2Tank</defName>
		<label>O2 Tank</label>
		<description>A tank for containing compressed O2.</description>
		<graphicData>
			<texPath>Things/Building/o2tank</texPath>
