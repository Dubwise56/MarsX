<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="MinifiedBuildingBase">
		<defName>Treadmill</defName>
		<label>treadmill</label>
		<thingClass>MarsX.Building_Treadmill</thingClass>
		<description>A treadmill is a device generally for walking or running while staying in the same place.</description>
		<graphicData>
			<texPath>Things/Building/treadmill/treadmill_0</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawRotated>false</drawRotated>
			<drawSize>(3,3)</drawSize>
		</graphicData>
		<uiIconPath>UI/treadmill</uiIconPath>
		<tickerType>Normal</tickerType>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Standable</passability>
		<pathCost>50</pathCost>
		<size>(1,1)</size>
		<rotatable>false</rotatable>
    <comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>150</basePowerConsumption>
			</li>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
		<costList>
			<Steel>50</Steel>
			<Plasteel>25</Plasteel>
			<ComponentIndustrial>4</ComponentIndustrial>
		</costList>
		<statBases>
			<MaxHitPoints>75</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<MarketValue>350</MarketValue>
			<WorkToBuild>550</WorkToBuild>
		</statBases>
		<designationCategory>Joy</designationCategory>
	</ThingDef>

	<ThingDef ParentName="BuildingBase">
		<defName>Basin</defName>
		<label>Basin</label>
		<description>Clean and simple bathroom basin. Instructions sold separately.</description>
		<thingClass>MarsX.Building_basin</thingClass>
		<graphicData>
			<drawSize>(3,3)</drawSize>
			<texPath>Things/Building/basin</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<damageData>
				<rect>(0.2,0.2,0.6,0.6)</rect>
			</damageData>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<statBases>
			<MaxHitPoints>75</MaxHitPoints>
			<WorkToBuild>550</WorkToBuild>
			<Flammability>1.0</Flammability>
		</statBases>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<hasInteractionCell>true</hasInteractionCell>

		<drawerType>MapMeshAndRealTime</drawerType>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
		<fillPercent>0.15</fillPercent>
		<costList>
			<Steel>15</Steel>
			<Plasteel>25</Plasteel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<comps>
			<li Class="MarsX.CompProperties_Pipe">
				<mode>Sewage</mode>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<pathCost>8</pathCost>
		<designationCategory>Joy</designationCategory>
		<rotatable>true</rotatable>
		<designationHotKey>Misc2</designationHotKey>
	</ThingDef>

	<ThingDef ParentName="BuildingBase">
		<defName>Shower</defName>
		<label>Shower</label>
		<description>Features a large 110mm 4 spray powered showerhead to counter low gravity including massage mode to relax those atrophied muscles!</description>
		<thingClass>MarsX.Building_Shower</thingClass>
		<graphicData>
			<drawSize>(2,1)</drawSize>
			<texPath>Things/Building/shower</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<damageData>
				<rect>(0.2,0.2,0.6,0.6)</rect>
			</damageData>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<size>(2,1)</size>
		<statBases>
			<MaxHitPoints>75</MaxHitPoints>
			<WorkToBuild>1300</WorkToBuild>
			<Flammability>1.0</Flammability>
		</statBases>

		<drawerType>MapMeshAndRealTime</drawerType>
		<fillPercent>0.15</fillPercent>
		<costList>
			<Steel>25</Steel>
			<Plasteel>55</Plasteel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<passability>Standable</passability>
		<interactionCellOffset>(0,0,1)</interactionCellOffset>
		<comps>
			<li Class="MarsX.CompProperties_Pipe">
				<mode>Sewage</mode>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<pathCost>8</pathCost>
		<designationCategory>Joy</designationCategory>
		<rotatable>true</rotatable>
		<designationHotKey>Misc2</designationHotKey>
	</ThingDef>

	<ThingDef ParentName="BuildingBase">
		<defName>Terlet</defName>
		<label>Toilet</label>
		<thingClass>MarsX.Building_terlet</thingClass>
		<graphicData>
			<texPath>Things/Building/terlet</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<damageData>
				<rect>(0.2,0.2,0.6,0.6)</rect>
			</damageData>
			<drawSize>(3,3)</drawSize>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Standable</passability>
		<statBases>
			<MaxHitPoints>75</MaxHitPoints>
			<WorkToBuild>950</WorkToBuild>
			<Flammability>1.0</Flammability>
		</statBases>
		<description>Sanitation fixture used for the disposal of human urine and feces.</description>
		<drawerType>MapMeshAndRealTime</drawerType>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
		<fillPercent>0.15</fillPercent>
		<costList>
			<Steel>25</Steel>
			<Plasteel>25</Plasteel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<hasInteractionCell>true</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<comps>
			<li Class="MarsX.CompProperties_Pipe">
				<mode>Sewage</mode>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<pathCost>8</pathCost>
		<designationCategory>Joy</designationCategory>
		<rotatable>true</rotatable>
		<designationHotKey>Misc2</designationHotKey>
	</ThingDef>

	<ThingDef ParentName="PipeBase">
		<defName>sewagePipe</defName>
		<label>Sewage pipe</label>
		<description>Sewage pipe for connecting a MIST to washing and toilet facilities.</description>
		<graphicData>
			<texPath>Things/Building/SewagePipe_Atlas</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<linkType>Basic</linkType>
			<linkFlags>
				<li>Custom5</li>
			</linkFlags>
			<damageData>
				<rect>(0,0.35,1,0.3)</rect>
			</damageData>
		</graphicData>
    <building>
      <blueprintGraphicData>
        <texPath>Things/Building/SewagePipe_Atlas</texPath>
      </blueprintGraphicData>
    </building>
    <uiIconPath>UI/SewagePipe</uiIconPath>
		<comps>	
			<li Class="MarsX.CompProperties_Pipe">
				<mode>Sewage</mode>
			</li>
		</comps>
		<designationCategory>Joy</designationCategory>
		<placeWorkers>
			<li>MarsX.PlaceWorker_SewagePipe</li>
		</placeWorkers>
	</ThingDef>

	<ThingDef ParentName="BuildingBase">
		<defName>SewageProcessing</defName>
		<label>MIST</label>
		<description>Mars In-Situ Sewage Treatment (MIST) Uses a multi-filtration process of wet oxidation and carbon filters to create a closed loop sewage system.</description>
		<thingClass>MarsX.Building_SewageProcessing</thingClass>
		<graphicData>
			<texPath>Things/Building/sewageProc</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(2,2)</drawSize>
			<damageData>
				<rect>(0.05,0.05,0.95,0.95)</rect>
			</damageData>
		</graphicData>
		<category>Building</category>
		<tickerType>Normal</tickerType>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
		<blockWind>true</blockWind>
		<castEdgeShadows>true</castEdgeShadows>
		<selectable>true</selectable>
		<pathCost>8</pathCost>
		<size>(2,2)</size>
		<costList>
			<Steel>155</Steel>
			<ComponentIndustrial>4</ComponentIndustrial>
		</costList>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<designationCategory>Joy</designationCategory>
		<rotatable>false</rotatable>
		<statBases>
			<MaxHitPoints>350</MaxHitPoints>
			<WorkToBuild>1000</WorkToBuild>
			<Flammability>0.0</Flammability>
			<Beauty>-12</Beauty>
		</statBases>
		<designationHotKey>Misc11</designationHotKey>
		<comps>
			<li Class="MarsX.CompProperties_Pipe">
				<mode>Water</mode>
			</li>
			<li Class="MarsX.CompProperties_Pipe">
				<mode>Sewage</mode>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>300</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
		<building>
			<soundAmbient>sewage_Ambience</soundAmbient>
		</building>
	</ThingDef>


</Defs>