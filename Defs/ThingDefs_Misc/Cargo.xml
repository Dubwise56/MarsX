	<ThingDef Name="BaseStorageBox" ParentName="ResourceBase">
		<defName>StorageBox</defName>
		<label>Storage Box</label>
		<description>A supply crate from Earth.</description>
		<thingClass>MarsX.StorageBox</thingClass>
		<graphicData>
			<texPath>Things/Building/StorageBox</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(2,2)</drawSize>
			<color>(255, 255, 255)</color>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<blockWind>true</blockWind>
		<castEdgeShadows>false</castEdgeShadows>
		<blockLight>false</blockLight>
		<inspectorTabs>
			<li>MarsX.ITab_ShipInventory</li>
		</inspectorTabs>
		<comps>
			<li Class="CompProperties_Transporter">
				<compClass>MarsX.CompCargo</compClass>
				<restEffectiveness>0</restEffectiveness>
				<massCapacity>300</massCapacity>
				<max1PerGroup>true</max1PerGroup>
				<canChangeAssignedThingsAfterStarting>true</canChangeAssignedThingsAfterStarting>
				<showOverallStats>true</showOverallStats>
			</li>
			<li>
				<compClass>CompColorable</compClass>
			</li>
		</comps>
		<statBases>
			<Mass>20</Mass>
			<MaxHitPoints>50</MaxHitPoints>
			<WorkToBuild>800</WorkToBuild>
			<Flammability>0.0</Flammability>
		</statBases>
		<drawGUIOverlay>true</drawGUIOverlay>
		<stackLimit>1</stackLimit>
		<thingCategories>
			<li>Storage</li>
		</thingCategories>
		<size>(1,1)</size>
		<rotatable>false</rotatable>
		<costList>
			<Steel>30</Steel>
		</costList>
		<building>
			<isMealSource>true</isMealSource>
			<isInert>true</isInert>
			<fixedStorageSettings>
				<filter>
					<categories>
					</categories>
				</filter>
			</fixedStorageSettings>
			<defaultStorageSettings>
				<filter>
					<categories>
					</categories>
				</filter>
			</defaultStorageSettings>
		</building>
	</ThingDef>

