<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <BiomeDef Name="BaseMoonBiome">
    <defName>MoonSurface</defName>
    <label>Moon Surface</label>
    <description>The surface of the Moon</description>
    <workerClass>MarsX.BiomeWorker_Moon</workerClass>
    <texture>Things/God</texture>
    <animalDensity>0</animalDensity>
    <plantDensity>0</plantDensity>
    <hasVirtualPlants>false</hasVirtualPlants>
    <settlementSelectionWeight>0.9</settlementSelectionWeight>
    <movementDifficulty>1</movementDifficulty>
    <forageability>0</forageability>
    <diseaseMtbDays>90</diseaseMtbDays>
    <diseases>
      <li>
        <diseaseInc>Disease_Flu</diseaseInc>
        <commonality>100</commonality>
      </li>
    </diseases>
    <terrainsByFertility>
      <li>
        <terrain>MoonDust</terrain>
        <min>-999</min>
        <max>999</max>
      </li>
    </terrainsByFertility>
    <terrainPatchMakers>
      <li>
        <perlinFrequency>0.028</perlinFrequency>
        <thresholds>
          <li>
            <terrain>MoonDust</terrain>
            <min>0.4</min>
            <max>999</max>
          </li>
        </thresholds>
      </li>
      <li>
        <perlinFrequency>0.027</perlinFrequency>
        <thresholds>
          <li>
            <terrain>MoonRock</terrain>
            <min>0.22</min>
            <max>999</max>
          </li>
        </thresholds>
      </li>
    </terrainPatchMakers>
    <baseWeatherCommonalities>
      <ClearMoon>1</ClearMoon>
    </baseWeatherCommonalities>
    <wildPlants>
    </wildPlants>
    <wildAnimals>
    </wildAnimals>
    <allowedPackAnimals>
    </allowedPackAnimals>
  </BiomeDef>

