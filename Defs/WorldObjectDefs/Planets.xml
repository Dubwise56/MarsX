  <MarsX.PlanetDef>
    <defName>Moon</defName>
    <label>Moon</label>
    <description>Earth's Moon is the first stepping stone into the solar system, a very short travel time for resupply and rescue missions makes the moon a safe starting point</description>
    <worldObjectClass>MarsX.WorldObject_Planet</worldObjectClass>
    <texture>World/MOON</texture>
    <Diameter>2158.8</Diameter>
    <Playable>false</Playable>
    <useDynamicDrawer>true</useDynamicDrawer>
    <selectable>true</selectable>
    <expandingIcon>false</expandingIcon>
    <expandingIconTexture>UI/hsbMark</expandingIconTexture>
    <expandingIconPriority>6</expandingIconPriority>
    <Divisions>5</Divisions>
    <RockTypes>
      <li>Basalt</li>
    </RockTypes>
  </MarsX.PlanetDef>

